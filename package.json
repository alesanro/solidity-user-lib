{
  "name": "solidity-user-lib",
  "version": "0.2.1",
  "description": "User contracts Shared Library",
  "main": "truffle.js",
  "scripts": {
    "test": "npx truffle test",
    "release": "standard-version",
    "compile": "npx truffle compile",
    "compile:all": "npx truffle compile --all",
    "testrpc": "npx ganache-cli -g 1 --gasLimit 8000000 --trace_request true --account='0x340e383de49af0dfaf69349eb1b9f73d055d7ae559826183bb1d2d893908d8b8,1000000000000000000000' --account='0x2ed950bc0ff7fc1f62aa3b302437de9763d81dcde4ce58291756f84748d98ce9,1000000000000000000000' --account='0xdaeb307eb13b4717d01d9f175ea3ed94374da8fefa52082379d2955579ce628a,1000000000000000000000' --account='0xce4b3dadb2e8e12032f27369e5ad4b04bb26d8ca0c468d2059ddf8c0491e2a63,1000000000000000000000' --account='0x756d8252500ae27255a4e822b4fadd9b8f7b4d16495dfe05af990f533baa9969,1000000000000000000000' --account='0x34da9c7053c4eb0ed869ac6773f8dfe9964e3a0140a42da2c55fa4a55f975e9f,1000000000000000000000' --account='0x46e5df8a291ff9112503a1007b0288f44132e4542397b4ca6415094393ef7cb9,1000000000000000000000' --account='0x1e3816bb73ad4a70ea3e7606f930e2d2d492ab9d5c26776656191b1be2ae0204,1000000000000000000000' --account='0x3822d9785cb9d7d26ff5535ae455010e4520854729267fcde16676a540af85ae,1000000000000000000000' --account='0x18c6db900c2d83112786a47d981dcac1311fbb8a96d6a850f2d095aa423d3f85,1000000000000000000000' | grep -Ev \"FilterSubprovider|eth_getFilterChanges\"",
    "lint:sol": "npx solium --dir contracts",
    "lint:js": "npx eslint . --fix",
    "lint": "npm run lint:sol && npm run lint:js",
    "coverage": "./node_modules/.bin/solidity-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.chronobank.io:SmartContracts/solidity-user-lib.git"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "loose": true
        }
      ]
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "keywords": [
    "smart-contracts",
    "laborx",
    "chronobank",
    "ethereum",
    "solidity"
  ],
  "dependencies": {
    "solidity-shared-lib": "git+ssh://git@gitlab.chronobank.io:SmartContracts/solidity-shared-lib.git#v0.2.3",
    "solidity-eventshistory-lib": "git+ssh://git@gitlab.chronobank.io:SmartContracts/solidity-eventshistory-lib.git#v0.1.2",
    "solidity-storage-lib": "git+ssh://git@gitlab.chronobank.io:SmartContracts/solidity-storage-lib.git#v0.2.1",
    "solidity-roles-lib": "git+ssh://git@gitlab.chronobank.io:SmartContracts/solidity-roles-lib.git#v0.1.1"
  },
  "devDependencies": {
    "ganache-cli": "6.1.4",
    "truffle": "4.1.13",
    "abi-decoder": "^1.0.9",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "web3-utils": "1.0.0-beta.35",
    "web3-eth-accounts": "1.0.0-beta.35",
    "eslint": "4",
    "solium": "1",
    "babel-eslint": "^8.2.5",
    "coveralls": "^3.0.1",
    "solidity-coverage": "^0.5.4",
    "standard-version": "^4.0.0"
  },
  "bugs": {
    "url": "https://gitlab.chronobank.io/SmartContracts/solidity-user-lib/issues"
